        "popper.js": "^1.15.0",
        "react": "^16.10.2",
        "react-dom": "^16.10.2"
    },
    "scripts": {
        "start": "node index.js",
        "unit": "karma start karma.conf.js ",
        "test": "jest --watchAll --coverage --coverageDirectory=test/react",
        "snapshots": "jest --watchAll --testPathPattern=lib/snapshots.js"
    },
    "jest": {
        "setupFilesAfterEnv": [
            "<rootDir>/jest-setup.js"
        ],
        "verbose": true,
        "testMatch": [
            "**/__tests__/**/*.[jt]s?(x)",
            "**/snapshots.js"
        ],
        "transform": {
            "^.+\\.js$": "babel-jest"
        },
        "modulePaths": [
            "<rootDir>/node_modules/"
        ],
        "coverageReporters": [
            "html"
        ],
        "collectCoverageFrom": [
            "src/components/**/*.js"
        ]
    }
